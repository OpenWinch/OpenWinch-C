{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "OpenWinch GDB",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/openwinch",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "miDebuggerPath": "${workspaceFolder}/.vscode/gdb-wrapper",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Activer l'impression en mode Pretty pour gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Make build"
    }
  ]
}